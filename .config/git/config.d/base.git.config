[init]
  defaultBranch = master
  templatedir = "~/dotfiles/git-config/.config/git/templates"

# Remove email address from global config:
# $ git config --global --unset-all user.email
[user]
  # name = issenn
  # email = "(none)"
  # Require setting user.name and email per-repo:
  # useConfigOnly = true

# Core variables
[core]
  # editor = vim
  autocrlf = input  # input for MacOS/Linux, true for Windows
  quotepath = false  # 不使用 \ 转义路径中文字符
  # excludesfile = ~/.gitignore
  # sshCommand = ssh -vvv
  # hooksPath = "~/.config/git/templates/hooks"

[program]
  pgp = gpg

[help]
  autocorrect = 1

[gui]
    encoding = utf-8
[i18n "commit"]
    encoding = utf-8
[i18n]
    logoutputencoding = utf-8

